    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@vitejs/plugin-vue": "^1.9.3",
    "eslint": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.1.1",
    "husky": "^7.0.4",
    "prettier": "^2.4.1",
    "sass": "^1.44.0",
    "stylelint": "^14.1.0",
    "stylelint-config-recommended-vue": "^1.0.0",
